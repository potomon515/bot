{
  "name": "kenibox-ss-tool",
  "version": "1.0.0",
  "description": "KeniBox SS Tool para Minecraft",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "minecraft",
    "tool",
    "kenibox",
    "screenshot",
    "jar"
  ],
  "author": "potomon5152",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.kenibox.sstool",
    "productName": "KeniBox SS Tool",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "asarUnpack": [
      "node_modules/**/*"
    ],
    "win": {
  "target": ["nsis", "portable"],
  "icon": "src/assets/icon.ico"
},
    "mac": {
      "target": "dmg",
      "icon": "src/assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "src/assets/icon.png"
    }
  }
}
